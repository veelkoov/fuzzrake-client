{
    "name": "veelkoov/fuzzrake-client",
    "version": "0.1.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Veelkoov",
            "email": "veelkoov@protonmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fuzzrake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fuzzrake\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "psr/simple-cache": "3.*",
        "symfony/http-client": "^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4",
        "composer/pcre": "^3.3",
        "friendsofphp/php-cs-fixer": "^3.88",
        "symfony/cache": "^7.3"
    },
    "scripts": {
        "pcf": "vendor/bin/php-cs-fixer fix --diff -vv --allow-risky=yes",
        "ps": "vendor/bin/phpstan analyse",
        "pu": "vendor/bin/phpunit"
    }
}
